configuration {
  modi: "window,run,ssh";
  font: "hack 10";
  sort: true;
  sorting-method: "normal";
  matching: "normal";
  tokenize: true;
  timeout {
    action: "kb-cancel";
    delay:  0;
  }
  filebrowser {
    directories-first: true;
    sorting-method:    "name";
  }
}@theme "/usr/share/rofi/themes/lb.rasi"
